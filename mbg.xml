<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_ 0.dtd">

<generatorConfiguration>

    <context id="DB2Tables" targetRuntime="MyBatis3">

        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/formu"
                        userId="root"
                        password="123">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <!--制定bean的位置-->
        <javaModelGenerator targetPackage="com.formu.bean"
                            targetProject="./dao/src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!--指定sql映射文件的位置-->
        <sqlMapGenerator targetPackage="mybatis/mapper"
                         targetProject="./dao/src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!--            制定dao接口生成的位置,mapper接口-->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.formu.mapper"
                             targetProject="./dao/src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
        <!--   制定每个表的生成策略 -->

<!--        <table tableName="formu_category" domainObjectName="Category"-->
<!--               enableCountByExample="false"-->
<!--               enableUpdateByExample="false"-->
<!--               enableDeleteByExample="false"-->
<!--               enableSelectByExample="false"-->
<!--               selectByExampleQueryId="false"/>-->
        <table tableName="formu_user" domainObjectName="User"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false">
            <columnOverride column="article_good" javaType="java.lang.String" jdbcType="VARCHAR" />
            <columnOverride column="pho" javaType="java.lang.String" jdbcType="VARCHAR" />
            <columnOverride column="friends" javaType="java.lang.String" jdbcType="VARCHAR" />
        </table>
<!--        <table tableName="formu_article" domainObjectName="Article"-->
